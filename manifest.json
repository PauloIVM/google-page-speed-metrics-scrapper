{
    "manifest_version": 3,
    "name": "Page-speed Metrics Tool",
    "description": "Tool to get main page-speed metrics",
    "version": "0.1",
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage",
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_popup": "popup.html"
    }
}